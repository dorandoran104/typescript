<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CategoryMapper">

  <insert id="insertAncestor">
    INSERT INTO category(
      name
      ,depth
    )VALUES(
      #{name}
      ,#{depth}
    )
  </insert>
</mapper>